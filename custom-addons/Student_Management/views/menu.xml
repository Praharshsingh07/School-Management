<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <menuitem id="menu_school_management" name="Student Management" sequence="1"/>
    <menuitem id="menu_student_management" name="More" parent="menu_school_management" sequence="2"/>

    <!-- Actions -->
    <record id="action_student" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">student.student</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="action_parent" model="ir.actions.act_window">
        <field name="name">Parents</field>
        <field name="res_model">student.parent</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_student" name="Students" parent="menu_student_management" action="action_student"/>
    <menuitem id="menu_parent" name="Parents" parent="menu_student_management" action="action_parent"/>
</odoo>
